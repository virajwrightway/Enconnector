<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <!-- instance : contain class name which function we want to run on defined time -->
        <!-- method : contain method name which run on defined time -->
        <job name="Wrightwaydigital_Enconnector_CronSync" instance="Wrightwaydigital\Enconnector\Cron\CronSync" method="execute">
            <schedule>* * * * *</schedule>
        </job>
    </group>
</config>
